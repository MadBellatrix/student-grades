{
"compilerOptions": {
 "target": "ES6",
 "module": "CommonJS",
 "outDir": "./dist",
 "rootDir": "./src",
 "strict": true,
 "esModuleInterop": true,
 "skipLibCheck": true,
 "noEmit": false,
 "moduleResolution": "node"
 },
 "include": ["src"], // Compiler soll nur Dateien in src berücksichtigen
 "exclude": ["node_modules"] // Ignoriert node_standardmäßig ausgeschlossen)
}
